 <div class="container my-5">
      <div class="jumbotron">
        <h1>SIGNUP</h1>
        <br />
        <form
          class="form"
          role="form"
          action="/register"
          method="post"
          autocomplete="off"
        >
          <div class="form-group">
            <input
              name="username"
              placeholder="Nombre"
              class="form-control my-3"
              type="text"
              required
            />
            <input
              name="email"
              placeholder="Email"
              class="form-control my-3"
              type="text"
              required
            />
            <input
              name="password"
              placeholder="Contrasenia"
              class="form-control my-3"
              type="password"
              required
            />
          </div>
          <div class="form-group">
            <input class="btn btn-success my-3" type="submit" value="Singup" />
          </div>
        </form>
      </div>
    </div>
            <div class="text-center"></div>
        <button class="btn btn-success my-3" onclick="goSignup()">
          LOGIN
        </button>
      </div>
    </div>

    <script>
      function redireccionar(pagina) {
        location.href = pagina;
      }

      function goSignup() {
        redireccionar("/login");
      }
    </script>